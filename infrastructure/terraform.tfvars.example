# =================================================================================
# ABSOLUTE TMS - Terraform Variables (Example)
# =================================================================================
# Copy this file to terraform.tfvars and fill in your actual values

# AWS Configuration
aws_region = "us-east-1"
environment = "production"
project_name = "absolute-tms"

# Domain Configuration
domain_name = "yourdomain.com"  # Replace with your actual domain

# Database Configuration
db_password = "your-secure-database-password-here"  # Use a strong password

# API Keys (Keep these secure!)
stripe_api_key = "sk_live_your_stripe_api_key"     # Your Stripe secret key
jwt_secret = "your-jwt-secret-key-here"             # Generate with: openssl rand -base64 32